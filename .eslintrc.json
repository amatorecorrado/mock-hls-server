{
    "env": {
        "node": true
    },
    "globals": {
        "Promise": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-cond-assign": 0,
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    },
    "extends": "eslint:recommended"
}
